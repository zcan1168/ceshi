.container {
    display: flex;
    flex-direction: column;
    background-color: rgb(255,255,255,0);
    color: #000;
    margin: 10px 30px;
}

.cell {
    position: relative;
    display: flex;
    flex-direction: row;
    width: 100%;
    height: 50px;
    font-size: 14px;
    line-height: 50px;
}

.key {
    position: absolute;
    left: 0;
    top: 0;
    color: #C1C1C1;
}

.value {
    position: absolute;
    right: 0;
    top: 0;
    color: #555;
}
