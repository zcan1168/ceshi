view {
    display:flex;
}
#container {
    height:100%;
    width:100%;
}
swiper {
    height:100%;
    width:100%;
}
image {
    width:100%;
    height:100%;
}
.indicators {
    justify-content: center;
    position:absolute;
    bottom:15px;
    width:100%;
    height:4px;
}
.indicator {
    width:15px;
    height:100%;
    margin-left:2px;
    margin-right:2px;
    border-radius:2px;
}
.indicator-normal {
    background-color:rgba(240,240,240,0.5);
}
.indicator-focus {
    background-color:#eee;
}
